{
  "name": "tarto-tech",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server-minimal.js",
    "dev": "nodemon server-minimal.js",
    "prod": "node server-minimal.js",
    "setup:indexes": "node scripts/runIndexCreation.js",
    "setup:production": "npm run setup:indexes && npm start",
    "health-check": "curl -f http://localhost:${PORT:-5000}/api/health || exit 1",
    "lint": "eslint . --ext .js --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js --ignore-path .gitignore --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "docker:build": "docker build -t tarto-server .",
    "docker:run": "docker run -p 5000:5000 --env-file .env tarto-server"
  },
  "keywords": ["taxi", "ride-sharing", "booking", "api"],
  "author": "Tarto Tech",
  "license": "ISC",
  "description": "Production-ready taxi booking server with geospatial features",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.6.0",
    "geolib": "^3.3.4",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.1",
    "multer": "^1.4.5-lts.2",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
